{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "allowJs": false,
        "sourceMap": false,
        "outDir": "./js",
        "noImplicitReturns": false,
        "allowUnreachableCode": false,
        "lib": [
            "es2015",
            "es2015.iterable",
            "dom"
        ]
    },
    "exclude": [
        "node_modules"
    ],
    "include": [
        "./ts/*"
    ]
}